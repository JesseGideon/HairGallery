        <ImageView
            android:id="@+id/imageView149"
            android:layout_width="122dp"
            android:layout_height="129dp"
            android:scaleType="fitXY"
            android:onClick="onclickImg5"
            app:srcCompat="@drawable/fixing5"
            app:layout_constraintLeft_toRightOf="@+id/imageView148"
