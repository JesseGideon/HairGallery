<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/BoysCollection"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.hpuser.hairgallery.BoysCollection">

    <TextView
        android:id="@+id/textView3"
        android:layout_width="138dp"
        android:layout_height="34dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="26dp"
        android:text="Round Cut"
        android:textColor="#000"
        android:textSize="20dp"
        android:textStyle="italic|bold"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintLeft_toRightOf="@+id/textView"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="26dp" />

    <ImageView
        android:id="@+id/lowCut"
        android:layout_width="171dp"
        android:layout_height="176dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/imageView5"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.152"
        app:srcCompat="@drawable/samplelowcut"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="172dp"
        android:layout_height="173dp"
        android:layout_marginTop="61dp"
        android:scaleType="fitXY"
        android:onClick="RoundCutClick"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/sampleafro"
        android:layout_marginRight="-8dp" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="187dp"
        android:layout_height="222dp"
        android:scaleType="fitXY"
        android:clickable="true"
        android:onClick="MohawlkClicked"
        app:srcCompat="@drawable/samplemohawk"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="@+id/textView2"
        android:layout_marginBottom="32dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintHorizontal_bias="0.566"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/lowCut" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="160dp"
        android:layout_height="32dp"
        android:text="Low Cut"
        android:textSize="20dp"
        android:clickable="true"
        android:textColor="#000"
        android:textStyle="italic|bold"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp"
        android:layout_marginLeft="9dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/lowCut"
        app:layout_constraintVertical_bias="0.421"
        android:layout_marginStart="16dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="205dp"
        android:layout_height="32dp"
        android:text="Mohawk Cut &amp; Galax "
        android:textSize="20dp"
        android:clickable="true"
        android:textColor="#000"
        android:textStyle="italic|bold"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/imageView6"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.64"
        app:layout_constraintVertical_bias="1.0"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp" />
</android.support.constraint.ConstraintLayout>
